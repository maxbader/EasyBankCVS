    "version": "0.2.0",
    "configurations": [
        {
            "name": "Add Header",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/easybank_csv_add_header.py",
            "args": [
                "input.csv",
                "header.csv"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Filter",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/easybank_csv_filter.py",
            "args": [
                "header.csv",
                "filtered.csv"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "FreeFinance",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/freefinance_csv.py",
            "args": [
                "filtered.csv",
                "freefinance.csv"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}"
        }
    ]
}